@{
    var adminName = Session["Login"];
}
@if (adminName != null)
{
    <!DOCTYPE html>
    <html lang="en">

    <head>

        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <meta name="description" content="">
        <meta name="author" content="">

        <title>SB Admin - Tables</title>

        <!-- Bootstrap core CSS-->
        <link href="~/Admin/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">

        <!-- Custom fonts for this template-->
        <link href="~/Admin/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">

        <!-- Page level plugin CSS-->
        <link href="~/Admin/vendor/datatables/dataTables.bootstrap4.css" rel="stylesheet">

        <!-- Custom styles for this template-->
        <link href="~/Admin/css/sb-admin.css" rel="stylesheet">

    </head>

    <body id="page-top">

        <nav class="navbar navbar-expand navbar-dark bg-dark static-top">

            <a class="navbar-brand mr-1" href="index.html">Admin Dashboard</a>

            <button class="btn btn-link btn-sm text-white order-1 order-sm-0" id="sidebarToggle" href="#">
                <i class="fas fa-bars"></i>
            </button>
        </nav>

        <div id="wrapper">

            <!-- Sidebar -->
            <ul class="sidebar navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" href="#">
                        <i class="fas fa-fw fa-tachometer-alt"></i>
                        <span>Karnel Travel</span>
                    </a>
                </li>
                <li class="nav-item active">
                    <a class="nav-link" href="@Url.Action("Admin", "Dashboard")">
                        <i class="fas fa-fw fa-table"></i>
                        <span>Admin</span>
                    </a>
                </li>
                <li class="nav-item active">
                    <a class="nav-link" href="@Url.Action("Hotel", "Dashboard")">
                        <i class="fas fa-fw fa-table"></i>
                        <span>Hotel</span>
                    </a>
                </li>
                <li class="nav-item active">
                    <a class="nav-link" href="@Url.Action("Resort", "Dashboard")">
                        <i class="fas fa-fw fa-table"></i>
                        <span>
                            Resort
                        </span>
                    </a>
                </li>
                <li class="nav-item active">
                    <a class="nav-link" href="@Url.Action("Restaurant", "Dashboard")">
                        <i class="fas fa-fw fa-table"></i>
                        <span>Restaurant</span>
                    </a>
                </li>
                <li class="nav-item active">
                    <a class="nav-link" href="@Url.Action("Tourist_Spots", "Dashboard")">
                        <i class="fas fa-fw fa-table"></i>
                        <span>Tourist spots</span>
                    </a>
                </li>
                <li class="nav-item active">
                    <a class="nav-link" href="@Url.Action("Travel", "Dashboard")">
                        <i class="fas fa-fw fa-table"></i>
                        <span>Travel</span>
                    </a>
                </li>
                <li class="nav-item active">
                    <a class="nav-link" href="@Url.Action("User", "Dashboard")">
                        <i class="fas fa-fw fa-table"></i>
                        <span>User Account</span>
                    </a>
                </li>
            </ul>

            <div id="content-wrapper">
                <div class="container-fluid">
                    @RenderBody()
                </div>
                <!-- /.container-fluid -->
                <!-- Sticky Footer -->
                <footer class="sticky-footer">
                    <div class="container my-auto">
                        <div class="copyright text-center my-auto">
                            <span>Copyright © Your Website 2018</span>
                        </div>
                    </div>
                </footer>
            </div>
            <!-- /.content-wrapper -->

        </div>
        <!-- /#wrapper -->
        <!-- Scroll to Top Button-->
        <a class="scroll-to-top rounded" href="#page-top">
            <i class="fas fa-angle-up"></i>
        </a>

        <!-- Logout Modal-->
        <div class="modal fade" id="logoutModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title" id="exampleModalLabel">Ready to Leave?</h5>
                        <button class="close" type="button" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">×</span>
                        </button>
                    </div>
                    <div class="modal-body">Select "Logout" below if you are ready to end your current session.</div>
                    <div class="modal-footer">
                        <button class="btn btn-secondary" type="button" data-dismiss="modal">Cancel</button>
                        <a class="btn btn-primary" href="login.html">Logout</a>
                    </div>
                </div>
            </div>
        </div>

        <!-- Bootstrap core JavaScript-->
        <script src="~/Admin/vendor/jquery/jquery.min.js"></script>
        <script src="~/Admin/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

        <!-- Core plugin JavaScript-->
        <script src="~/Admin/vendor/jquery-easing/jquery.easing.min.js"></script>

        <!-- Page level plugin JavaScript-->
        @*<script src="~/Admin/vendor/datatables/jquery.dataTables.js"></script>*@
        <script src="~/Admin/vendor/datatables/dataTables.bootstrap4.js"></script>

        <!-- Custom scripts for all pages-->
        <script src="~/Admin/js/sb-admin.min.js"></script>

        <!-- Demo scripts for this page-->
        <script src="~/Admin/js/demo/datatables-demo.js"></script>

    </body>

</html>

}
else {
    Response.Redirect("Login");
}